    <link href="http://fonts.googleapis.com/css?family=Allerta+Stencil:400,700,900:normal" rel="stylesheet" type="text/css">
    <!-- Bootstrap Core CSS -->
    <link href="Content/Mobile/bootstrap/css/bootstrap.min.css" type="text/css" rel="stylesheet" />
